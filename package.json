{
  "name": "matters-avatar",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "develop:start": "hardhat node",
    "develop:console": "hardhat console --network localhost",
    "develop:deploy": "hardhat run --network localhost scripts/deploy.js",
    "develop:mint": "hardhat run --network localhost scripts/mint.js",
    "rinkeby:console": "hardhat console --network rinkeby",
    "rinkeby:deploy": "hardhat run --network rinkeby scripts/deploy.js",
    "rinkeby:mint": "hardhat run --network rinkeby scripts/mint.js",
    "store": "node ./scripts/store.js",
    "build": "hardhat compile",
    "test": "hardhat test",
    "prepare": "husky install",
    "lint": "solhint 'contracts/**/*.sol'",
    "format": "prettier --write \"{,!(node_modules|build|coverage|artifacts|assets|cache)/**/}*.{sol,js,jsx,ts,tsx,json}\""
  },
  "keywords": [],
  "author": "Matters Lab",
  "license": "Apache 2.0",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@openzeppelin/contracts": "^4.3.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "ethers": "^5.4.7",
    "hardhat": "^2.6.5",
    "husky": "^7.0.2",
    "nft.storage": "^3.2.2",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "web3": "^1.5.3"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": false
  }
}
