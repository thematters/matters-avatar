{
  "name": "matters-avatar",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "develop:start": "ganache-cli --deterministic",
    "develop:console": "truffle console --network development",
    "develop:deploy": "truffle migrate --network development",
    "develop:mint": "truffle exec --network development ./scripts/mint.js",
    "rinkeby:console": "truffle console --network rinkeby",
    "rinkeby:deploy": "truffle migrate --network rinkeby",
    "rinkeby:mint": "truffle exec --network rinkeby ./scripts/mint.js",
    "store": "node ./scripts/store.js",
    "build": "truffle compile",
    "test": "truffle test",
    "prepare": "husky install",
    "lint": "solhint 'contracts/**/*.sol'",
    "format": "prettier --write \"{,!(node_modules|build|coverage)/**/}*.{sol,js,jsx,ts,tsx,json}\""
  },
  "keywords": [],
  "author": "",
  "license": "Apache 2.0",
  "devDependencies": {
    "@openzeppelin/contracts": "^4.3.2",
    "@truffle/hdwallet-provider": "^1.5.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "ganache-cli": "^6.12.2",
    "husky": "^7.0.2",
    "nft.storage": "^3.2.2",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "truffle": "^5.4.10",
    "web3": "^1.5.3"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": false
  }
}
